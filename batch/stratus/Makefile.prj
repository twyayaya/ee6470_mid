#
# Dependency Generation rule for BEH module
#
$(BDW_OBJDIR)/SobelFilter.d :  ../SobelFilter.cpp
	@if [ ! -d $(BDW_OBJDIR) ]; then mkdir -p $(BDW_OBJDIR); fi
	@echo "Generating dependencies for $< "
	@set -e; $(BDW_CCDEP) -MM -MG $(BDW_CCFLAGS)  ../SobelFilter.cpp \
	| sed 's|SobelFilter\.o[ :]*|$(BDW_OBJDIR)/SobelFilter.o $(BDW_OBJDIR)/SobelFilter.d : |' $(BDW_DEP_FILTER)  > $@

$(BDW_WORKLIB)/wrappers/SobelFilter_trace.h : $(BDW_WORKLIB)/wrappers/SobelFilter_trace.h.updated

$(BDW_WORKLIB)/wrappers/SobelFilter_trace.h.updated :  ../SobelFilter.cpp $(BDW_LOGOPTIONS_FILE)
	$(STRATUS_HOME)/bin/bdw_tracegen -module SobelFilter  
	@touch $(BDW_WORKLIB)/wrappers/SobelFilter_trace.h.updated



$(BDW_WORKLIB)/objs/SobelFilter.o:  ../SobelFilter.cpp $(BDW_WORKLIB)/wrappers/SobelFilter_wrap.cpp $(BDW_WORKLIB)/wrappers/SobelFilter_wrap.h $(BDW_WORKLIB)/wrappers/SobelFilter_cosim.h $(BDW_WORKLIB)/wrappers/SobelFilter_trace.h 
	@if [ ! -d $(BDW_OBJDIR) ]; then mkdir -p $(BDW_OBJDIR); fi
	$(BDW_CC)  $(BDW_CCFLAGS)   -o $(BDW_WORKLIB)/objs/SobelFilter.o  $(BDW_WORKLIB)/wrappers/SobelFilter_wrap.cpp

.INTERMEDIATE : SobelFilter_wrap.h

SobelFilter_wrap.h : $(BDW_WORKLIB)/wrappers/SobelFilter_wrap.h

$(BDW_WORKLIB)/wrappers/SobelFilter_wrap.cpp $(BDW_WORKLIB)/wrappers/SobelFilter_wrap.h $(BDW_WORKLIB)/wrappers/SobelFilter_cosim.h $(BDW_WORKLIB)/wrappers/SobelFilter_cosim.v   : $(BDW_WRAPDIR)/SobelFilter.updated

$(BDW_WRAPDIR)/SobelFilter.updated :  ../SobelFilter.cpp 
	@if [ ! -d $(BDW_WRAPDIR) ]; then mkdir -p $(BDW_WRAPDIR); fi
	$(STRATUS_HOME)/bin/bdw_wrapgen -project project.tcl -module SobelFilter "-I$(BDW_WRAPDIR) $(BDW_LIB_DASHI) $(BDW_CCOPTIONS) $(BDW_EXTRA_CCFLAGS)"
	@touch $(BDW_WRAPDIR)/SobelFilter.updated
	$(STRATUS_HOME)/bin/bdw_tracegen -module SobelFilter  
	@touch $(BDW_WORKLIB)/wrappers/SobelFilter_trace.h.updated
$(BDW_WORKLIB)/wrappers/SobelFilter_wrap.d :  ../SobelFilter.cpp $(BDW_WORKLIB)/wrappers/SobelFilter_wrap.h
	@if [ ! -d $(@D) ]; then mkdir -p $(@D); fi
	@set -e; $(BDW_CCDEP) -MM -MG $(BDW_CCFLAGS)   ../SobelFilter.cpp \
		| sed 's|SobelFilter\.o[ :]*|$(BDW_WRAPDIR)/SobelFilter.updated $(BDW_WORKLIB)/wrappers/SobelFilter_wrap.d :  |' $(BDW_DEP_FILTER) > $(BDW_WORKLIB)/wrappers/SobelFilter_wrap.d 




